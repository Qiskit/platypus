{
  "name": "@qiskit/platypus",
  "version": "0.1.1",
  "description": "Project Platypus",
  "repository": "https://github.ibm.com/IBM-Q-Software/project-platypus",
  "license": "Apache-2.0",
  "main": "server/app.ts",
  "scripts": {
    "clean:studio": "rm -rf node_modules/@mathigon/studio/public",
    "clean:public": "rm -rf public",
    "clean:work": "rm -rf working",
    "clean": "npm-run-all --parallel clean:studio clean:public clean:work",
    "build:vue": "VUE_CLI_SERVICE_CONFIG_PATH=$PWD/frontend/vue/vue.config.js vue-cli-service build --target lib --name textbooklib --dest public/lib frontend/vue/main.ts",
    "build:nb": "ts-node -s converter/converter.ts",
    "build:studio": "mgon-build --assets --minify --search",
    "build": "npm-run-all build:nb build:vue build:studio",
    "watch:vue": "VUE_CLI_SERVICE_CONFIG_PATH=$PWD/frontend/vue/vue.config.js vue-cli-service build --no-clean --watch --target lib --name textbooklib --dest public/lib frontend/vue/main.ts",
    "watch:studio": "mgon-build --assets --watch",
    "watch": "npm-run-all --parallel watch:vue watch:studio",
    "start": "ts-node -I public -P server/tsconfig.json server/app.ts",
    "start-dev": "nodemon --watch 'server/**/*.ts' --exec 'ts-node -I public -P server/tsconfig.json server/app.ts'",
    "dev": "npm-run-all --parallel watch start-dev",
    "thumbnails": "mgon-build --thumbnails",
    "screenshots": "mgon-screenshots --output screenshots",
    "postbuild:studio": "ts-node -s converter/postbuild.ts"
  },
  "engines": {
    "node": "14.16.0"
  },
  "dependencies": {
    "@carbon/colors": "^10.22.0",
    "@carbon/icons-vue": "^10.24.0",
    "@carbon/themes": "^10.26.0",
    "@carbon/vue": "^2.37.1",
    "@ibm/plex": "^5.1.3",
    "@mathigon/boost": "^0.6.14",
    "@mathigon/euclid": "^0.6.7",
    "@mathigon/studio": "github:vabarbosa/studio#platypus",
    "@webcomponents/webcomponentsjs": "^2.5.0",
    "carbon-components": "^10.30.0",
    "carbon-web-components": "^1.12.0",
    "lit-element": "^2.4.0",
    "lit-html": "^1.3.0",
    "ts-node": "9.1.1",
    "typescript": "4.2.3",
    "vue": "^3.0.0",
    "vue-class-component": "^8.0.0-0",
    "vue-router": "^4.0.1",
    "vuedraggable": "^4.0.1"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config-typescript": "^5.0.0",
    "@types/jsdom": "^16.2.10",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "eslint": "^7.18.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-vue": "^7.5.0",
    "fs-extra": "^10.0.0",
    "js-yaml": "^4.1.0",
    "jsdom": "^16.5.3",
    "node-sass": "^5.0.0",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "sass-loader": "^10.1.0",
    "sass-mq": "^5.0.1",
    "tslib": "^2.2.0"
  }
}
